{
  "manifest_version": 2,

  "name": "Print current page",
  "description": "Works like CTRL + P",
  "version": "1.0",

  "browser_action": {
    // "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": [
    "activeTab", "tabs", "http://*/*", "https://*/*"
  ]
}
